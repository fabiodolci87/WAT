<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nespoli.dolci.wat" >

    <permission
        android:name="com.javapapers.android.mymap.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.javapapers.android.mymap.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".AddComment"
            android:label="@string/title_activity_add_comment"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ReadComments"
            android:label="@string/title_activity_read_comments"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".JSONParser"
            android:label="@string/title_activity_jsonparser" >
        </activity>
        <activity
            android:name=".JoinCreate"
            android:label="@string/title_activity_join_create"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".JoinGroup"
            android:label="@string/title_activity_join_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".CreateGroup"
            android:label="@string/title_activity_create_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".UserMenu"
            android:label="@string/title_activity_user_menu"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".CoordMenu"
            android:label="@string/title_activity_coord_user"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Itinerary"
            android:label="@string/title_activity_itinerary"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ItineraryCoord"
            android:label="@string/title_activity_itinerary_coord"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".AddItinerary"
            android:label="@string/title_activity_add_itinerary"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" >
        </activity>
        <activity
            android:name=".ListOfUser"
            android:label="@string/title_activity_list_of_user"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".PicFromUrl"
            android:label="@string/title_activity_prova"
            android:noHistory="true"
            android:screenOrientation="portrait">
>
        </activity>
        <activity
            android:name=".InsertPic"
            android:label="@string/title_activity_insertpic"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".TakePhoto"
            android:label="@string/title_activity_prova_cap"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".HelpActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
